@* @page
@model BulkyWebRazor_Temp.Pages.Shared._NotificationModel
@{
}
 *@

@if (TempData["success"] != null)
{
    <script type="text/javascript">
        toastr.success("@TempData["success"]", "Success", { timeOut: 3000, closeButton: true, progressBar: true });
    </script>
}

@if (TempData["error"] != null)
{
    <script type="text/javascript">
        toastr.error("@TempData["error"]", "Error", { timeOut: 3000, closeButton: true, progressBar: true });
    </script>
}
